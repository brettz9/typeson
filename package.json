{
  "name": "typeson",
  "version": "5.13.0",
  "description": "Preserves types over JSON, BSON or socket.io",
  "main": "./dist/typeson-commonjs2.js",
  "browser": "./dist/typeson.js",
  "module": "./dist/typeson-esm.js",
  "scripts": {
    "prepublishOnly": "yarn",
    "eslint": "eslint --ext js,md,html .",
    "start": "static -p 8092",
